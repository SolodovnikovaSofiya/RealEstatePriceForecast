<Page x:Class="RealEstatePriceForecast.Pages.MapPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:RealEstatePriceForecast.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="550" d:DesignWidth="1000"
      Title="Карта">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <TextBlock Text="Средняя цена квадратного метра жилья по районам Москвы" Grid.Row="0" Width="520"/>
        <WebBrowser x:Name="OSMMapBrowser" Loaded="OSMMapBrowser_Loaded" Grid.Row="1"/>
    </Grid>
</Page>
